<h4>
    <%= obj["First Name"] + " " + obj["Last Name"] %>
</h4>             
<p>
    <%= obj["Address"] %><br />
    <!-- <%= obj["Phone"] %><br /> -->
    <% if (obj["Subspecialty"]) { %>
        <strong>Subspecialty: <%= obj["Subspecialty"] %></strong><br />
    <% } %><br />
    <% if (obj["URL"]) { %>
        <a href='<%= obj["URL"] %>' target='_blank'>Website</a>
    <% } %>
</p>

<% if (obj["Cost"]) { %>
    <p>
        <strong>Cost</strong><br />
        <%= obj["Cost"] %>
    </p>
<% } %>

<% if (obj["Hours"]) { %>
    <p>
        <strong>Hours</strong><br />
        <%= obj["Hours"] %>
    </p>
<% } %>